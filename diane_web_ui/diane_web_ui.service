[Unit]
Description=Diane Web UI
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
User=diane
WorkingDirectory=/home/diane/diane
EnvironmentFile=/home/diane/diane/.env
Environment=PATH=/home/diane/diane/.venv/bin:/usr/bin:/bin
ExecStart=/home/diane/diane/.venv/bin/python3 /home/diane/diane/diane_web_ui.py
Restart=on-failure
RestartSec=5

[Install]
WantedBy=multi-user.target
